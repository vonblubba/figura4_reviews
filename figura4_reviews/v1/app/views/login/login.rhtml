<div class="depot-form" >
	<fieldset>
	<legend>Please Log In</legend>
		<%= start_form_tag %>
			<p>
				<label for="name" >Name:</label>
				<%= text_field_tag :name, params[:name] %>
			</p>
			<p>
				<label for="password" >Password:</label>
				<%= password_field_tag :password, params[:password] %>
			</p>
			<p>
				<%= submit_tag "Login" %>
			</p>
		<%= end_form_tag %>
	</fieldset>
</div>
